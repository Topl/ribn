{
  "name": "Ribn",
  "description": "A lightweight flutter wallet to interact with the Topl blockchain",
  "version": "0.0.1",
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "background": {
    "service_worker": "/dist/background.bundle.js",
    "type": "module"
  },
  "action": {
    "default_icon": {
      "16": "/icons/Icon-192.png",
      "32": "/icons/Icon-192.png",
      "48": "/icons/Icon-192.png",
      "128": "/icons/Icon-192.png"
    }
  },
  "icons": {
    "16": "/icons/Icon-192.png",
    "32": "/icons/Icon-192.png",
    "48": "/icons/Icon-192.png",
    "128": "/icons/Icon-192.png"
  },
  "permissions": ["storage"],
  "web_accessible_resources": [
    {
      "resources": ["/dist/injected.bundle.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["/dist/content.bundle.js"],
      "type": "module",
      "run_at": "document_start"
    }
  ],

  "manifest_version": 3
}
